{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "Serilog.AspNetCore": "Warning",
        "Discord": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ]
  },
  "OpenTelemetry": {
    "Tracing": {
      "SamplingRatio": 1.0,
      "EnableConsoleExporter": true
    }
  }
}
