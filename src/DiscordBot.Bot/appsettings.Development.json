{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "Serilog.AspNetCore": "Warning",
        "Discord": "Debug"
      }
    }
  },
  "OpenTelemetry": {
    "Tracing": {
      "SamplingRatio": 1.0,
      "EnableConsoleExporter": true
    }
  }
}
